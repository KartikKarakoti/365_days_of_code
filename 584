import java.util.Scanner;

public class EvenOdd {
    static void main() {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter a number:");
        int i=sc.nextInt();
        if(i%2==0)
        {
            System.out.print("even");
        }

         else
        {
            System.out.print("odd");
        }
    }
}
